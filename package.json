    "typescript": "^5.2.2",
    "eslint": "^8.47.0",
    "@typescript-eslint/parser": "^6.5.2",
    "@typescript-eslint/eslint-plugin": "^6.5.2"
  },
  "dependencies": {
    "axios": "^1.6.6"
  },
  "repository": {
